{
  "version": 2,
  "builds": [
    {
      "src": "Bongocat.html",
      "use": "@vercel/static"
    },
    {
      "src": "Img/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Bongocat.html"
    }
  ]
}
